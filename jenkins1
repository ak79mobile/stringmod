pipeline {
    
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'committed Building the application...'
            }
        }
        stage('test') {
            steps {
                echo 'committed Testing the application...'
            }
        }
        stage('deploy') {
            steps {
                echo ' committed  Deploying the application...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'committed  Deploying....'
            }
        }
    }
}
